						"exec": [
							"pm.test(\"Invalid api key header should return 401 status\", function () {\r",
							"    pm.response.to.have.status(401);\r",
							"});"
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "GET",
				"header": [
					{
						"key": "API-Key",
						"value": "atest123cg",
						"type": "text"
					},
					{
						"key": "Content-Type",
						"value": "application/json",
						"type": "text"
					}
				],
				"url": {
					"raw": "{{url}}/rest/developers",
					"host": [
						"{{url}}"
					],
					"path": [
						"rest",
						"developers"
					]
