<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE Configuration>
<Configuration xmlns:xi="http://www.w3.org/2001/XInclude" status="warn" monitorInterval="300" name="CHPL-Service-local" packages="">
    <Appenders>
        <Console name="STDOUT" target="SYSTEM_OUT">
            <PatternLayout>
                <Pattern>%d{yyyy-MM-dd HH:mm:ss} %-5p %c{1}:%L - %m%n</Pattern>
            </PatternLayout>
        </Console>
    </Appenders>
    <Loggers>
        <Logger name="gov.healthit.chpl" level="DEBUG" additivity="false">
            <AppenderRef ref="STDOUT" />
        </Logger>
    </Loggers>
</Configuration>
