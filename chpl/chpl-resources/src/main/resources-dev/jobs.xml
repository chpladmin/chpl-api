    <pre-processing-commands>
        <delete-triggers-in-group>triggerJob</delete-triggers-in-group>
        <delete-triggers-in-group>interruptJob</delete-triggers-in-group>
        <!-- For OCD-2944 -->
        <delete-triggers-in-group>*</delete-triggers-in-group>
        <!-- END OCD-2944 -->
        <!-- Delete the below pre-processing command(s) after activity has happened on PROD -->
        <delete-job>
            <name>addCriteriaTo2015ListingsJob</name>
            <group>systemJobs</group>
        </delete-job>
        <delete-job>
            <name>addCriteriaToSingleListingJob</name>
            <group>subordinateJobs</group>
        </delete-job>
        <delete-job>
            <name>addCriteriaToSinglePendingListingJob</name>
            <group>subordinateJobs</group>
        </delete-job>
        <delete-job>
            <name>modifyTestFunctionalityJob</name>
            <group>systemJobs</group>
        </delete-job>
        <delete-job>
            <name>updateMacraMeasuresJob</name>
            <group>systemJobs</group>
        </delete-job>
        <!-- End delete pre-processing command(s). -->
        <!-- Delete the below pre-processing command(s) after PROD push -->
        <!-- End delete pre-processing command(s). -->
