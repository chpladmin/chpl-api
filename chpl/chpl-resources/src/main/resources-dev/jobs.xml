            <recover>false</recover>
            <job-data-map>
                <entry>
                    <key>authorities</key>
                    <value>ROLE_ADMIN;ROLE_ONC</value> <!-- Semi-colon delimited string -->
                </entry>
                <entry>
                    <key>frequency</key>
                    <value>DAILY</value><!-- NEEDS TO BE DAILY -->
                </entry>
            </job-data-map>
        </job>
