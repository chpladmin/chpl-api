                <name>qaAllBrokenSurveillanceACBEmailJob</name>
                <group>qaTrigger</group>
                <job-name>All Broken Surveillance Rules Report</job-name>
                <job-group>chplJobs</job-group>
                <job-data-map>
                    <entry>
                        <key>email</key>
                        <value>amore@ainq.com</value>
                    </entry>
                </job-data-map>
