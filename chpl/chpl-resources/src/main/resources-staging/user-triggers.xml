                    <entry>
                        <key>email</key>
                        <value>amore@ainq.com</value>
                    </entry>
                </job-data-map>
                <misfire-instruction>MISFIRE_INSTRUCTION_DO_NOTHING</misfire-instruction>
                <cron-expression>0 0 11 ? * WED</cron-expression>
            </cron>
        </trigger>

        <trigger>
            <cron>
                <name>qaAllBrokenSurveillanceEmailJob</name>
